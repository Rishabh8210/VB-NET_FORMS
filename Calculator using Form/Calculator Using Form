Public Class Form1
    Private Sub Button1_Click(sender As Object, e As EventArgs) Handles Button1.Click
        Dim fNum As Decimal
        Dim sNum As Decimal
        fNum = Val(TextBox1.Text)
        sNum = Val(TextBox2.Text)

        Dim Result As Decimal
        Result = fNum + sNum
        TextBox3.Text = Format(Val(Result), "0.00")'use to set Precission
    End Sub

    Private Sub Button2_Click(sender As Object, e As EventArgs) Handles Button2.Click
        Dim fNum As Decimal
        Dim sNum As Decimal

        fNum = Val(TextBox1.Text)
        sNum = Val(TextBox2.Text)

        Dim Result As Decimal
        Result = fNum - sNum
        TextBox3.Text = Format(Val(Result), "0.00")'use to set Precission
    End Sub

    Private Sub Button3_Click(sender As Object, e As EventArgs) Handles Button3.Click
        Dim fNum As Decimal
        Dim sNum As Decimal

        fNum = Val(TextBox1.Text)
        sNum = Val(TextBox2.Text)

        Dim Result As Decimal
        Result = fNum * sNum
        TextBox3.Text = Format(Val(Result), "0.00")'use to set Precission
    End Sub

    Private Sub Button4_Click(sender As Object, e As EventArgs) Handles Button4.Click
        Dim fNum As Decimal
        Dim sNum As Decimal

        fNum = Val(TextBox1.Text)
        sNum = Val(TextBox2.Text)

        Dim Result As Decimal
        Result = fNum / sNum
        TextBox3.Text = Format(Val(Result), "0.00")'use to set Precission
    End Sub
End Class
